apiVersion: kubeadm.k8s.io/v1alpha2
kind: MasterConfiguration
kubernetesVersion: v1.12.1
apiServerCertSANs:
- "k8s.yunwei.edu"
api:
    controlPlaneEndpoint: "k8s.yunwei.edu:8443"
etcd:
  local:
    extraArgs:
      listen-client-urls: "https://127.0.0.1:2379,https://192.168.31.32:2379"
      advertise-client-urls: "https://192.168.31.32:2379"
      listen-peer-urls: "https://192.168.31.32:2380"
      initial-advertise-peer-urls: "https://192.168.31.32:2380"
      initial-cluster: "master1.yunwei.edu=https://192.168.31.31:2380,master2.yunwei.edu=https://192.168.31.32:2380"
      initial-cluster-state: existing
      name: master2.yunwei.edu
    serverCertSANs:
      - master2.yunwei.edu
      - 192.168.31.32
    peerCertSANs:
      - master2.yunwei.edu
      - 192.168.31.32
networking:
    podSubnet: "10.244.0.0/16"
