   #pass uwsgi python web app
   location /myapp {
       include uwsgi_params;
       uwsgi_param SCRIPT_NAME /myapp;
   #or# uwsgi_pass 127.0.0.1:3031;
       uwsgi_pass myapp;
   }
   
   location /student {
       include uwsgi_params;
       uwsgi_param SCRIPT_NAME /student;
   #or# uwsgi_pass 127.0.0.1:3031;
       uwsgi_pass student;
   }
 
   location ^~ /static/ {
       alias /uwsgi/static/;
   }
